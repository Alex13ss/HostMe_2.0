<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.password">1</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/oms</property>

		<property name="hibernate.hbm2ddl.auto">create</property>

		<mapping class="com.softserve.edu.model.Contact" />
		<mapping class="com.softserve.edu.model.Feedback" />
		<mapping class="com.softserve.edu.model.Hosting" />
		<mapping class="com.softserve.edu.model.Image" />
		<mapping class="com.softserve.edu.model.Language" />
		<mapping class="com.softserve.edu.model.Request" />
		<mapping class="com.softserve.edu.model.Role" />
		<mapping class="com.softserve.edu.model.Status" />
		<mapping class="com.softserve.edu.model.SystemProperties" />
		<mapping class="com.softserve.edu.model.User" />
	</session-factory>
</hibernate-configuration>